        "55da0587-5936-4a13-a042-eb96ec75df15": {
          "type": "collapsible_tab",
          "settings": {
            "heading": "Pricing",
            "open": false,
            "content": "<p>The price will vary due to the following factors: <\/p><ul><li>Design<\/li><li>Fabric<\/li><li>Sewing<\/li><li>Logo type<\/li><li>Stone\/sequence<\/li><li>Amount of products ordered.<\/li><\/ul><p>You can read more about our prices and see examples <a href=\"\/pages\/pricing\" title=\"Pricing\">here<\/a>. <\/p><p>Fill out the form below to request an offer<\/p>",
            "page": "",
            "custom_liquid": ""
          }
        },
        "collapsible_tab_PztHKw": {
          "type": "collapsible_tab",
          "settings": {
            "heading": "Delivery",
            "open": false,
            "content": "<p>Standard delivery is 11 weeks from when your order is placed.<\/p>",
            "page": "",
