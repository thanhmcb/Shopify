    "4f10bebd-1b29-4195-af73-cf24c053e083": {
      "type": "media-with-text",
      "blocks": {
        "template--18272984301870__4f10bebd-1b29-4195-af73-cf24c053e083-1686645824a0c05d5c-0": {
