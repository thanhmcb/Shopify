        "3e1e1b33-d247-4acc-ab0b-d57b20763e73": {
          "type": "richtext",
          "settings": {
            "content": "<h5>Work with us<\/h5><p>To become an OEM partner like Specialized, or discuss other opportunities with us please <a href=\"\/pages\/contact\" title=\"Contact us\">get in touch<\/a><\/p>"
          }
        }
