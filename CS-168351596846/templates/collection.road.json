        "48e4bb33-c59e-4372-9260-6c4dc6d9a637": {
          "type": "image",
          "settings": {
            "image": "shopify:\/\/shop_images\/custom_resized_3f82c7b2-c794-438e-b430-fa16d0599d51.jpg",
