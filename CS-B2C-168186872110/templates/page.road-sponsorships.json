    "9ca69751-b6c0-4b98-b466-646d88c8c98f": {
      "type": "media-with-text",
      "blocks": {
        "template--19634502697262__9ca69751-b6c0-4b98-b466-646d88c8c98f-1687202135d53ea819-0": {
          "type": "image",
          "settings": {
            "image": "shopify:\/\/shop_images\/1-jpg-1703423168_kopier.jpg",
            "media_width": 50,
            "media_position": "start",
            "text_position": "place-self-center text-center",
            "icon": "none",
            "icon_width": 48,
            "subheading": "",
            "title": "",
            "content": "<h5>Soudal Quick-Step<\/h5><p><br\/>Few teams have made winning as integrated a part of their DNA as Soudal Quick-Step. The Belgian squad boasts a roster with proven winners like two-time world champion Julian Alaphilippe, Tour de France stage winner Kasper Asgreen, and current ITT World Champion Remco Evenepoel (just to name just a few) who makes sure The Wolfpack is always represented in the front of the races. We've been supporting the team for a decade and are happy to confirm that they'll be riding our BB's for the 2024 season as well.<\/p><p>© Soudal Quick-Step \/ Getty Images<\/p>",
            "link_text": "",
            "link_url": "",
            "background": "rgba(0,0,0,0)",
            "background_gradient": "",
            "text_color": "#000000",
            "text_gradient": "",
            "button_background": "",
            "button_text_color": ""
          }
        }
      },
