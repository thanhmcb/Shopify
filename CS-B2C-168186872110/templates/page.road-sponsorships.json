    "media_with_text_7Bb6XL": {
      "type": "media-with-text",
      "blocks": {
        "image_LLKpc3": {
          "type": "image",
          "settings": {
            "image": "shopify:\/\/shop_images\/large-trainingAGladies-8320.jpg",
            "media_width": 50,
            "media_position": "end",
            "text_position": "place-self-start-center text-center",
            "icon": "none",
            "icon_width": 48,
            "subheading": "",
            "title": "",
            "content": "<h5>AG Insurance – Soudal <\/h5><p>Founded by Natascha den Ouden in 2019, AG Insurance-Soudal has a strong commitment to developing a sustainable professional women's team with a focus on talent development. Thanks to an excellent 2023 season that saw the Belgian squad rack up 15 total wins, they've joined the WorldTour for the 2024 season, and we're happy to report that the team will ride our OSPWs and BBs for this year's races.<\/p>",
            "link_text": "",
            "link_url": "",
            "background": "#fafafa",
            "background_gradient": "",
            "text_color": "#000000",
            "text_gradient": "",
            "button_background": "",
            "button_text_color": ""
          }
