    "e6e99b14-c77b-45e9-90a4-ebb385bf75e6": {
      "type": "media-with-text",
      "blocks": {
        "template--18123980112174__e6e99b14-c77b-45e9-90a4-ebb385bf75e6-16777524124b3d06ef-0": {
