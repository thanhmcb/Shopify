<script src="{{ product.metafields.custom.size_guide.value.source_file.value | asset_url }}" defer></script>
<script src="{{ product.metafields.custom.size_guide.value.collection_handles.value | asset_url }}" defer></script>
<script>
  document.addEventListener(
    'DOMContentLoaded',
    function () {
      let initialized = false;
      let firstValue;
      let secondValue;
      var errorMsg1 = "{{ 'size-guide.error1' | t }}";
      var errorMsg2 = "{{ 'size-guide.error2' | t }}";
      var yourSize = "{{ 'size-guide.your_size' | t }}";
      var collectionText = "{{ 'size-guide.collection' | t }}";
      document.getElementById('first').addEventListener('keyup', function () {
        var valid = validateFirst(this.value);
        firstValue = valid ? Number(this.value) : null;
        handleValidation(this, document.getElementById('firstError'), validateFirst, this.value, errorMsg1);
      });

      document.getElementById('second').addEventListener('keyup', function () {
        var valid = validateSecond(firstValue, this.value);
        secondValue = valid ? Number(this.value) : null;
        handleValidation(
          this,
          document.getElementById('secondError'),
          () => validateSecond(firstValue, this.value),
          this.value,
          errorMsg2
        );
      });

      document.getElementById('next').addEventListener('click', next);

      document.getElementById('first').addEventListener('keypress', function (event) {
        if (event.key === 'Enter') {
          next();
        }
      });

      document.getElementById('second').addEventListener('keypress', function (event) {
        if (event.key === 'Enter') {
          next();
        }
      });

      function handleValidation(inputElement, errorMessageElement, isValid, value, errorMessage) {
        if (!isValid(value)) {
          inputElement.style.borderColor = 'red';
          inputElement.classList.add('invalid');
          errorMessageElement.textContent = errorMessage;
        } else {
          inputElement.style.borderColor = '';
          inputElement.classList.remove('invalid');
          errorMessageElement.textContent = '';
        }
      }
      function next() {
        // If first step
        if (document.getElementsByClassName('first')[0].classList.contains('d-none') === false) {
          var valid = validateFirst(document.getElementById('first').value);
          firstValue = valid ? Number(document.getElementById('first').value) : null;
          handleValidation(
            document.getElementById('first'),
            document.getElementById('firstError'),
            validateFirst,
            document.getElementById('first').value,
            errorMsg1
          );
          if (!valid) return;
      const section = document.querySelector('.size-guide-modal .second .video-section');
        if (section) {
        setTimeout(() => {
        section.click();
        }, 500);
        } else {
         console.error('Error: .size-guide-modal or .video-section not found.');
        }
      }
        // If second step
        else if (document.getElementsByClassName('second')[0].classList.contains('d-none') === false) {
          var valid = validateSecond(firstValue, document.getElementById('second').value);
          secondValue = valid ? Number(document.getElementById('second').value) : null;
          handleValidation(
            document.getElementById('second'),
            document.getElementById('secondError'),
            () => validateSecond(firstValue, document.getElementById('second').value),
            document.getElementById('second').value,
            errorMsg2
          );
          if (!valid) return;
        }

        let firstData = searchFirst(firstValue);
        if (firstData == null) return;

        document.getElementsByClassName('first')[0].classList.add('d-none');
        document.getElementsByClassName('second')[0].classList.remove('d-none');

        document.getElementById('second').focus();

        setMinAndMax(firstData);

        let secondData = searchSecond(firstValue, secondValue);
        if (secondData == null) return;

    const sizeDefinition = firstData.size + '' + secondData.size;
    const foundCollection = sizeHandles[firstData.size + '' + secondData.size]

    const foundSizeText = yourSize+'<div class="size">' + sizeDefinition + '</div>';
    const foundCollectionText = foundCollection ? '<div class="sizeLink">{{ 'size-guide.or' | t }}<a class="btn btn--secondary" href="' + foundCollection + '">{{ 'size-guide.collection' | t }}</a></div>' : ''

    document.getElementsByClassName("result-text")[0].innerHTML = '<div class="sizeText">' + foundSizeText + '</div>';
    document.getElementsByClassName("product-text")[0].innerHTML = foundCollectionText;

    document.getElementsByClassName('second')[0].classList.add('d-none');
    document.getElementsByClassName('next')[0].classList.add('d-none');
    document.getElementsByClassName('result')[0].classList.remove('d-none');
    document.getElementsByClassName('reset')[0].classList.remove('d-none');

    document.getElementById('first').blur();
    document.getElementById('second').blur();
      }
      function resetForm() {
        // Reset the data variables
        firstValue = null;
        secondValue = null;
        document.getElementById('first').value = '';
        document.getElementById('second').value = '';

        document.getElementsByClassName('first')[0].classList.remove('d-none');
        document.getElementsByClassName('second')[0].classList.add('d-none');
        document.getElementsByClassName('result')[0].classList.add('d-none');
        document.getElementsByClassName('reset')[0].classList.add('d-none');
        document.getElementsByClassName('next')[0].classList.remove('d-none');

        document.getElementById('first').focus();
      }
     document.getElementById('reset').addEventListener('click', function (event) {
        resetForm();
      });

      function setMinAndMax(firstData) {
        if (firstData != null) {
          document.getElementsByClassName('second-description')[0].innerHTML =
            'Min: ' + firstData.second[0].min + 'cm - Max: ' + firstData.second[firstData.second.length - 1].max + 'cm';
        document.getElementsByClassName('second-input')[0].placeholder = firstData.second[0].min;
        }

        if (!initialized) {
          document.getElementsByClassName('first-description')[0].innerHTML =
            'Min: ' + size_guide_data[0].min + 'cm - Max: ' + size_guide_data[size_guide_data.length - 1].max + 'cm';
          document.getElementsByClassName('first-input')[0].placeholder = size_guide_data[0].min;
        }
      }
      function searchFirst(val) {
        if (val == null) return null;
        const valAsNumber = Number(val);
        for (let i = 0; i < size_guide_data.length; i++) {
          const element = size_guide_data[i];
          if (valAsNumber < element.min || valAsNumber > element.max) continue;

          return element;
        }
        return null;
      }

      function validateFirst(val) {
        const foundData = searchFirst(val);
        if (foundData == null) return false;
        return true;
      }

      function searchSecond(firstVal, secondVal) {
        if (secondVal == null) return null;

        const foundData = searchFirst(firstVal);
        if (foundData == null) return null;

        const valAsNumber = Number(secondVal);
        for (let i = 0; i < foundData.second.length; i++) {
          const element = foundData.second[i];
          if (valAsNumber < element.min || valAsNumber > element.max) continue;

          return element;
        }
        return null;
      }

      function validateSecond(firstVal, secondVal) {
        const foundData = searchSecond(firstVal, secondVal);
        if (foundData == null) return false;
        return true;
      }

      function init() {
        document.getElementById('first').focus();
        setMinAndMax();
        initialized = true;
      }


      init();
    },
    false
  );
</script>
<style>
  .size-guide-wrapper {
    display: flex;
    flex-direction: column;
    margin-bottom: 2rem;
  }

  .size-guide-wrapper > * {
    margin: 0;
  }

  .metafield-rich_text_field p:last-child {
    margin: 0;
  }

  .size-guide-wrapper h1,
  .size-guide-wrapper h2,
  .size-guide-wrapper h3,
  .size-guide-wrapper h4,
  .size-guide-wrapper h5,
  .size-guide-wrapper h6 {
    margin: 0;
  }

  .invalid {
    border-color: red;
  }

  .error-message {
    position: absolute;
    top: calc(100% - 0.5em);
    font-size: 0.8rem;
    color: red;
  }

  .gap-1 {
    gap: 1rem;
  }

  .gap-2 {
    gap: 2rem;
  }

  .d-none {
    display: none !important;
  }

  .column {
    flex-direction: column;
  }

  .size-wrapper {
    display: flex;
    flex-direction: column;
    gap: 2rem;
    margin-bottom: 2rem;
  }

  .result {
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .result-text {
    font-size: calc(4 * var(--font-scale) + var(--base-text-size));
    font-family: var(--heading-font-family);
    font-style: var(--heading-font-style);
    font-weight: var(--heading-font-weight);
    color: rgb(var(--footer-text-col));
  }

  .sizeText {
    display: flex;
    flex-direction: column;
    gap: 16px;
    line-height: 1;
    margin-bottom: 26px;
  }

  .sizeLink {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    gap: 30px;
    color: #5C5C5C;
    font-style: italic;
    font-size: 13px;
    letter-spacing: 0.06em;
  }

  .reset {
    display: flex;
    flex-direction: column;
    margin-top: -8px;
  }

  .reset .product-text {
    margin-bottom: 36px;
  }

  .reset-text {
    color: #5C5C5C;
    text-transform: uppercase;
    cursor: pointer;
    text-decoration-color: #5C5C5C !important;
  }

  .result-wrapper {
    position: relative;
    display: flex;
    flex-direction: column;
    width: 100%;
    align-items: center;
    justify-content: center;
    background: var(--footer-bg);
    color: rgb(var(--footer-text-col));
    padding: 36px;
    min-height: 275px;
    text-align: center;
  }

  .result-wrapper:after {
    content: '';
    position: absolute;
    background: inherit;
    margin: 0 auto;
    width: calc(100% + 88px);
    z-index: -1;
    height: 100%;
  }

  .result-wrapper .btn {
    background-color: #bf3c68;
    color: #FFFFFF;
    text-transform: uppercase;
  }

  .result-wrapper .icon--type-ruler {
    rotate: 225deg;
  }

  .step {
    margin-bottom: 1rem;
    line-height: 1;
  }

  .size {
    font-weight: bold;
    font-size: 45px;
  }

  .left,
  .right {
    flex: 1;
    display: flex;
    flex-direction: column;
  }

  .right-wrapper {
    display: flex;
    flex-direction: column;
    height: 100%;
    gap: 1rem;
  }

  .wrapper-text {
    display: flex;
    gap: 1rem;
  }

  .wrapper-text svg {

    min-width: 60px;
  }

  .input-row {
    display: flex;
    flex-direction: column;
    position: relative;
  }

  .input-row input {
    padding: 0;
    padding-top: .3em;
    border: 0;
    border-bottom: 1px var(--input-border-color) solid;
  }

  .input-row input:hover {
    border: 0;
    border-bottom: 1px var(--input-border-color-hover) solid
  }

  .input-row input:active,
  .input-row input:focus {
    border: 0;
    border-bottom: 1px var(--input-border-color-active) solid;
    box-shadow: unset;
  }

  ::placeholder {
    opacity: 0.25;
  }

  .first-description,
  .second-description {
    color: rgb(var(--text-color) / .4);
    margin-top: -1rem;
  }

  .next {
    display: flex;
    justify-content: center;
  }

  .next .btn {
    min-width: 245px;
  }

  @media (min-width: 768px) {
    .size-wrapper {
      flex-direction: row;
    }

    .wrapper-text {
      gap: 2rem;
    }

    .step {
      margin-bottom: 2rem;
    }

    .size-guide-wrapper {
      margin-bottom: 44px;
    }

    .size-wrapper {
      margin-bottom: 44px;
    }

    .right-wrapper {
      gap: 2rem;
    }
  }
</style>
